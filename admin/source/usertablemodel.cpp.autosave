//------------------------------------------------------------------------------

QModelIndex UserTableModel::index(int row, int column, const QModelIndex &parent) const
{
    if (!parent.isValid()) {
      QList<int> ids = _conta->productIds(_currentCategoryId);
      if (row >= 0 && row < ids.size() && column >= 0 && column <= info) {
        return createIndex(row, column, ids[row]);
      }
    }
  
    return QModelIndex();
}

